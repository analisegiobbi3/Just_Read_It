<form class="login-form">
  <form class="login-form text-center mb-5 col-lg6 col-md-6 col-sm-10"></form>
    <h2>Login</h2>
  <div class="form-group">
    <label for="exampleInputUsername1">Username</label>
    <input type="text" class="form-control" id="username-login" placeholder="Enter Username">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" class="form-control" id="password-login" placeholder="Enter Password">
  </div>
  <button type="submit" class="btn btn-primary pb-6"
  id="loginSubmitBtn">Submit</button>
  <input type="checkbox"><span> Remember me</span>
</form>

